<a class='card-link' href="./<%- game.name.toLowerCase().replace(' ', '_') %>.ejs">
<section class='card'>
  <h2 class='title section'><%- game.name %></h2>
  <table class='info section'>
    <thead>
      <tr>
        <th colspan='2' class='title'>Game Info</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td align='left'>Days drawn:</td>
        <td align='right'><%- game.daysDrawn.join(', ') %></td>
      </tr>
      <tr>
        <td align='left'>Last draw date:</td>
        <td align='right'><%- game.lastDrawDate %></td>
      </tr>
      <tr>
        <td align='left'>Analysis Start Date:</td>
        <td align='right'><%- game.analysisStartDate %></td>
      </tr>
      <tr>
        <td align='left'>Field Number Range:</td>
        <td align='right'>1 to <%- game.fieldNumMax %></td>
      </tr>
        <td align='left'>Bonus Ball Range:</td>
        <td align='right'>1 to <%- game.bonusBallMax %></td>
      </tr>
    </tbody>
  </table>
  <div id='<%- game.shortName _%>-picks' class='section'>
    <h4>Picks</h4>
    <% let overdueNums = [...game.fieldNumsOverdue.numbers] %>
    <% while (overdueNums.length > 4) { %>
    <div><%- overdueNums.splice(0, 5).sort().join(', ') %></div>
    <% } %>
    <div><%- overdueNums.splice(0, overdueNums.length).sort().join(', ') %></div>
  </div>
</section>
</a>